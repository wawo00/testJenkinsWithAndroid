        <attr name="halfround_color" format="color" />
        <attr name="rectangle_color1" format="color" />
    </declare-styleable>
    <declare-styleable name="CustomRelativeLayout">
        <attr name="round_radius" format="dimension" />
        <attr name="round_color" format="color" />
        <attr name="rectangle_color" format="color" />
        <attr name="dashWidth1" format="dimension" />
        <attr name="lineWidth1" format="dimension" />
